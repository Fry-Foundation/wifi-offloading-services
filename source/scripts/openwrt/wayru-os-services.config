
config wayru_os_services 'wayru_os_services1'
	# enabled
	# Default: 1 (enabled)
	# Set to 0 to disable wayru-os-services
	option enabled '1'
	###########################################################

	# main_api
	option main_api 'https://api.internal.wayru.tech'
	###########################################################

	# accounting api
	option accounting_enabled '1'
	option accounting_interval '300'
	option accounting_api 'https://wifi.internal.api.wayru.tech'
	###########################################################

	# access task
	option access_interval '10800'
	##########################################################

	# device status task
	option device_status_interval '120'
	##########################################################

	# setup task
	option setup_interval '120'
	##########################################################

	#console_log_level
	option console_log_level '1'
	##########################################################

	# monitoring
	option monitoring_enabled '1'
	option monitoring_interval '900'
	##########################################################

    # speed test
    option speed_test_enabled '1'
    option speed_test_interval '10800'
    option speed_test_backhaul_attempts '3'
    option speed_test_latency_attempts '4'
	option speed_test_file_size '0.3'
    ##########################################################

	# device context
	option device_context_interval '900'
	##########################################################

    # site clients
    option site_clients_interval '60'
    ##########################################################
