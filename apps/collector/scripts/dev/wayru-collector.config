config wayru_collector 'wayru_collector'
		option enabled '1'
		option logs_endpoint 'http://localhost:8080/v1/logs'
		option console_log_level '4'

		# Batching configuration (optimized for local development)
		option batch_size '5'
		option batch_timeout_ms '3000'
		option queue_size '50'

		# HTTP configuration (shorter timeouts for local testing)
		option http_timeout '10'
		option http_retries '1'
		option reconnect_delay_ms '2000'

		# Development settings
		option dev_mode '1'
