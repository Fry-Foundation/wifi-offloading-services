
config wayru_os_services 'wayru_os_services1'
	# enabled
	# Default: 1 (enabled)
	# Set to 0 to disable wayru-os-services
	option enabled '1'
	###########################################################

	# main_api
	option main_api 'https://api.wayru.tech'
	###########################################################

	# accounting api
	option accounting_enabled '1'
	option accounting_interval '300'
	option accounting_api 'https://wifi.api.wayru.tech'
	###########################################################

	# access task
	option access_interval '10800'
	##########################################################

	# device status task
	option device_status_interval '120'
	##########################################################

	# setup task
	option setup_interval '120'
	##########################################################

	#console_log_level
	option console_log_level '1'
	##########################################################

	# monitoring
	option monitoring_enabled '1'
	option monitoring_interval '20'
	##########################################################
